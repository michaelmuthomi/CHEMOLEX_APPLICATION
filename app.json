{
  "expo": {
    "name": "Refnet",
    "slug": "refnetapplication",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/Logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.michaelmuthomi.refnet"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.michaelmuthomi.refnet",
      "permissions": []
    },
    "web": {
      "bundler": "webpack",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "aa41b4a8-fb85-458f-8f40-070ba618ce2b"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "refnetapplication"
  }
}
